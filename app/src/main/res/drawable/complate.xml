<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="4dp"
        android:top="32dp"
        android:left="2.5dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#FDFDFD"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="50dp"
        android:height="4dp"
        android:top="25dp"
        android:left="18dp"
        android:right="20dp">
        <rotate
            android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="50dp"
        android:height="50dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</layer-list>